<!DOCTYPE flight_plan SYSTEM "../flight_plan.dtd">

<flight_plan alt="50" ground_alt="0" lat0="55.4719715" lon0="10.4153435" max_dist_from_home="340" name="Basic Odense" security_height="25">
  <waypoints>
    <waypoint name="HOME" x="-8.8" y="10.5"/>
    <waypoint alt="15.0" name="AF" x="39.6" y="172.2"/>
    <waypoint alt="0.0" name="TD" x="50.2" y="71.9"/>
    <waypoint name="_BASELEG" x="-70.5" y="232.6"/>
    <waypoint name="CLIMB" x="-71.8" y="166.3"/>
    <waypoint name="C" x="-65.1" y="120.8"/>
    <waypoint name="E" x="-23.8" y="253.2"/>
    <waypoint name="S" x="-10.6" y="83.1"/>
    <waypoint name="STDBY" x="-18.5" y="155.7"/>
    <waypoint name="A1" x="-119.8" y="142.0"/>
    <waypoint name="A2" x="-112.1" y="-5.0"/>
    <waypoint name="A3" x="-37.5" y="0.6"/>
    <waypoint name="A4" x="-30.9" y="149.9"/>
    <waypoint name="S5" x="34.4" y="57.7"/>
    <waypoint name="S6" x="31.3" y="188.2"/>
    <waypoint name="S7" x="79.4" y="190.6"/>
    <waypoint name="S8" x="79.4" y="60.2"/>
  </waypoints>
  <sectors>
    <sector name="PolySecto1r">
      <corner name="A1"/>
      <corner name="A2"/>
      <corner name="A3"/>
      <corner name="A4"/>
    </sector>
    <sector name="PolySector2">
      <corner name="S5"/>
      <corner name="S6"/>
      <corner name="S7"/>
      <corner name="S8"/>
    </sector>
  </sectors>
  <modules>
    <module name="gvf_module"/>
    <module name="distributed_circular_formation"/>
    <module name="collective_tracking_control"/>
  </modules>
  <exceptions>
    <exception cond="dist2_to_home > (max_dist_from_home*max_dist_from_home)" deroute="Circle_standby"/>
  </exceptions>
  <blocks>
    <block name="Wait GPS">
      <set value="1" var="autopilot.kill_throttle"/>
      <while cond="!GpsFixValid()"/>
    </block>
    <block name="Holding point">
      <set value="1" var="autopilot.kill_throttle"/>
      <attitude roll="0" throttle="0" vmode="throttle"/>
    </block>
    <block group="home" key="t" name="Takeoff" strip_button="Takeoff (wp CLIMB)" strip_icon="takeoff.png">
      <exception cond="GetPosAlt() > GetAltRef()+25" deroute="Circle_standby"/>
      <set value="0" var="autopilot.kill_throttle"/>
      <set value="0" var="autopilot.flight_time"/>
      <set value="80" var="nav_radius"/>
      <go from="HOME" pitch="15" throttle="1.0" vmode="throttle" wp="CLIMB"/>
    </block>
    <block group="home" key="Ctrl+a" name="Circle_standby" strip_button="Standby" strip_icon="home.png">
      <call_once fun="NavVerticalAutoThrottleMode(0.0)"/>
      <call_once fun="NavVerticalAltitudeMode(flight_altitude, 0.0)"/>
      <set value="50" var="flight_altitude"/>
      <call_once fun="gvf_set_direction(-1)"/>
      <call fun="gvf_ellipse_wp(WP_STDBY, nav_radius, nav_radius, 0)"/>
    </block>
    <block group="start" name="Circle" strip_button="C">
      <call_once fun="NavVerticalAutoThrottleMode(0.0)"/>
      <call_once fun="NavVerticalAltitudeMode(flight_altitude, 0.0)"/>
      <call_once fun="gvf_set_direction(-1)"/>
      <call fun="gvf_ellipse_wp(WP_C, gvf_ellipse_par.a, gvf_ellipse_par.b, gvf_ellipse_par.alpha)"/>
    </block>
    <block name="Distributed_circular">
      <call fun="distributed_circular(WP_C)"/>
    </block>
    <block name="Poly Survey N-S">
      <call_once fun="photogrammetry(4,1280,1024,81,35,80,135,5,4.13);"/>
      <call_once fun="JeVois_setting()"/>
      <call_once fun="gvf_nav_survey_polygon_setup(WP_A1,4,180,70)"/>
      <call fun="gvf_nav_survey_polygon_run()"/>
      <deroute block="Circle_standby"/>
    </block>
    <block name="Poly Survey S-N">
      <call_once fun="photogrammetry(4,1280,1024,81,35,80,135,5,4.13);"/>
      <call_once fun="JeVois_setting()"/>
      <call_once fun="gvf_nav_survey_polygon_setup(WP_S5,4,0,70)"/>
      <call fun="gvf_nav_survey_polygon_run()"/>
      <deroute block="Circle_standby"/>
    </block>
    <block group="land" name="Land Right AF-TD" strip_button="Land right (wp AF-TD)" strip_icon="land-right.png">
      <set value="DEFAULT_CIRCLE_RADIUS" var="nav_radius"/>
      <deroute block="land"/>
    </block>
    <block group="land" name="Land Left AF-TD" strip_button="Land left (wp AF-TD)" strip_icon="land-left.png">
      <set value="-DEFAULT_CIRCLE_RADIUS" var="nav_radius"/>
      <deroute block="land"/>
    </block>
    <block name="land">
      <call_once fun="nav_compute_baseleg(WP_AF, WP_TD, WP__BASELEG, nav_radius)"/>
      <circle radius="nav_radius" until="NavCircleCount() > 0.5" wp="_BASELEG"/>
      <circle radius="nav_radius" until="And(NavQdrCloseTo(DegOfRad(baseleg_out_qdr)-(nav_radius/fabs(nav_radius))*10), 10 > fabs(GetPosAlt() - WaypointAlt(WP__BASELEG)))" wp="_BASELEG"/>
    </block>
    <block name="final">
      <exception cond="GetAltRef() + 10 > GetPosAlt()" deroute="flare"/>
      <go from="AF" hmode="route" vmode="glide" wp="TD"/>
    </block>
    <block name="flare">
      <go approaching_time="0" from="AF" hmode="route" throttle="0.0" vmode="throttle" wp="TD"/>
      <attitude roll="0.0" throttle="0.0" until="FALSE" vmode="throttle"/>
    </block>
  </blocks>
</flight_plan>
